    "nodemon": "^3.1.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "openai": "^4.68.1",
    "prettier": "^3.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.4.0",
