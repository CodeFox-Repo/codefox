{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:backend": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "lint": {
      "outputs": [],
      "dependsOn": [
        "^lint"
      ]
    },
    "format": {
      "outputs": []
    },
    "start": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    }
  }
}